<div class="container">
  <div class="row search-result">
    <%= @athletes.each do |athlete| %>
      <div class="offset-2 col-md-10 member">
        <%= athlete.full_name %>
      </div>
    <% end %>
    <% if @pagy.pages > 1 %>
      <div class="pag-bar mt-5 pt-5">
        <%== pagy_bootstrap_nav(@pagy) %>
      </div>
    <% end %>
  </div>
</div>